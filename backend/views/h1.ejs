<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/homePage.css"/>
    <link rel="stylesheet" href="/css/dialog.css"/>
    <link rel="stylesheet" href="/css/navBar.css">
</head>

<body>
    <nav>
        <input id="search" type="text" placeholder="Search..."/>
        <ul id="list">
            <!-- dunamic li -->
        </ul>
    </nav>
    <div class="container">
        <div class="left">
            <div class="sidebar-profile-box">
                <div class="images">
                    <img class="outerImage" src="<%= user.profilePicture && user.profilePicture.data ? `data:${user.profilePicture.contentType};base64,${user.profilePicture.data.toString('base64')}` : 'data:image/jpeg;base64,' %>" alt="Profile Picture">
                    <div class="innerImage">
                        <img src="<%= user.profilePicture && user.profilePicture.data ? `data:${user.profilePicture.contentType};base64,${user.profilePicture.data.toString('base64')}` : 'data:image/jpeg;base64,' %>" alt="Profile pic">
                    </div>
                </div>
                
                <div class="sidebar-profile-info">
                    <h1><%= user.userName || "unknown" %></h1>
                    <h3><%= user.bio %></h3>
                    <a href="/render/edit">Edit</a>

                    <button class="openModalBtn" data-modal="modal1">Create Post</button>
                    <div id="modal1" class="modal">
                        <div class="modal-content">
                            <span class="close-btn">&times;</span>
                            <h2>Create Post</h2>
                            <form action="/post/addPost" method="post" enctype="multipart/form-data">
                                <label for="image">Image:</label>
                                <input type="file" id="image" name="image" required>
                                <br>
                                <label for="caption">Caption:</label>
                                <input type="text" id="caption" name="caption" placeholder="Caption" required>
                                <br>
                                <input type="submit" value="Post">
                            </form>
                        </div>
                    </div>    
                    <a href="/view/<%= user._id %>/profile">Profile</a>
                    <a href="/render/profile"><button>Create a proofile</button></a>
                    <a href="/render/chat/<%= user._id%>">Chat</a>
                    <ul>
                        <li>Your post <span><%= user.posts.length %></span></li>
                        <li>Your Connections<span><%= user.connection.length %></span></li>
                    </ul>
                </div>
                <div class="sidebar-profile-link">
                    <a href="#"><img src="https://static.thenounproject.com/png/3810268-200.png">My Posts</a>
                    <a href="#"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQz8nBgmjDJvdYvtak3qAQnvuETh5_s0j7Suw&s">Try Premium</a>
                </div>
            </div>
        </div>
        <div class="right">
            <div id="top">
                <div id="posts">
                    <% if (posts && posts.length > 0) { %>
                        <% posts.forEach(post => { %>
                            <div class="post">
                                <div class="photoNav">
                                    <div class="navInfo">
                                        <% if(post.author.profilePicture.data) { %>
                                            <img src="<%= `data:${post.author.profilePicture.contentType};base64,${post.author.profilePicture.data.toString('base64')}` %>" alt="Profile">
                                        <% } else { %>
                                            <img src="data:image/jpeg;base64," alt="profile" />
                                        <% } %>
                                        <div class="info">
                                            <div class="name"><%= post.author.userName %></div>
                                            <p class="location"><%= post.caption %></p>
                                        </div>
                                    </div>
                                    <button class="openModalBtn" data-modal="<%= post._id %>">...</button>
                                    <div id="<%= post._id %>" class="modal">
                                        <div class="modal-content">
                                            <span class="close-btn">&times;</span>
                                            <h2>Welcome</h2>
                                            <% if(post.author._id.toString() == user._id.toString()) { %>
                                                <a href="/post/delete/<%= post._id %>">
                                                    <button>Delete post</button>
                                                </a>                                                
                                            <% }else { %>
                                                <% if(user.connection.includes(post.author._id.toString())) { %>
                                                    <form action="/user/connection/<%= post.author._id %>" method="post">
                                                        <input type="submit" value="Disconnect"/>
                                                    </form>
                                                <%} else {%>
                                                    <form action="/user/connection/<%= post.author._id %>" method="post">
                                                        <input type="submit" value="Connect"/>
                                                    </form>
                                                <%}%>
                                            <% } %>
                                        </div>
                                    </div>  
                                </div>
                                <div class="photoPost">
                                    <img src="<%= `data:${post.image.contentType};base64,${post.image.data.toString('base64')}` %>" alt="Post">
                                </div>
                                
                                
                                
                                <% if(post.likes.includes(user.id.toString())) { %>
                                    <a href="/post/<%= post._id %>/dislike" ><button>disLike </button> <%= post.likes.length %> </a>
                                <%} else {%>
                                    <a href="/post/<%= post._id %>/like" ><button>Like </button> <%= post.likes.length %> </a>
                                <%}%>
                            </div>
                        <% }) %>
                    <% } else { %>
                        <p>No posts available.</p>
                    <% } %>
                    
                </div>
            </div>
            <div id="bottom">
                <p>Scroll</p>
            </div>
        </div>
    </div>
    <script src="/javascripts/dialog.js" ></script>
    <script src="/javascripts/search.js"></script>
</body>

</html>